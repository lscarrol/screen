{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "installCommand": "pip3 install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/login",
      "dest": "api/app.py",
      "methods": ["POST"]
    },
    {
      "src": "/api/login_with_session",
      "dest": "api/app.py",
      "methods": ["POST"]
    },
    {
      "src": "/api/validate_2fa",
      "dest": "api/app.py",
      "methods": ["POST"]
    },
    {
      "src": "/api/test",
      "dest": "api/app.py",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ]
}