{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "GOOGLE_APPLICATION_CREDENTIALS": "auth.json"
  }
}